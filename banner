<script type="text/javascript">
	var imagens = ["1.png", "2.png", "3.png", "4.png", "5.png", "6.png", "7.png", "8.png", "9.png"];
	var imagematual = 0;

	function trocaimagem() {
 		imagematual = (imagematual + 1) % 9;
	document.querySelector('.gif img'). src = imagens[imagematual];
	}
	setInterval(trocaimagem, 1900);	
</script>

